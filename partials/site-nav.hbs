<nav class="site-nav">
    <div class="site-nav-left">
        {{^is "home"}}
            {{#if @blog.logo}}
                <a class="site-nav-logo" href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="{{@blog.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@blog.url}}">{{@blog.title}}</a>
            {{/if}}
        {{/is}}
        {{#if @blog.navigation}}
            {{navigation}}
        {{/if}}
    </div>
    <div class="site-nav-right">
        <div class="social-links">
            {{#if @blog.facebook}}
                <a class="social-link social-link-fb" href="{{facebook_url @blog.facebook}}" target="_blank" rel="noopener">{{> "icons/facebook"}}</a>
            {{/if}}

            {{! add by Choin for telegram icon}}
            <a class="social-link social-link-telegram" href="https://t.me/coineast" target="_blank" rel="noopener">{{> "icons/telegram"}}</a>

            {{#if @blog.twitter}}
                <a class="social-link social-link-tw" href="{{twitter_url @blog.twitter}}" target="_blank" rel="noopener">{{> "icons/twitter"}}</a>
            {{/if}}

            {{! add by Choin for line icon}}
            <a class="social-link social-link-line" href="https://line.me/R/ti/p/%40pqi5087i" target="_blank" rel="noopener">{{> "icons/line"}}</a>

        </div>
    </div>
</nav>
